import java.util.Scanner;
public class exe4 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int R, G, B;
		
		System.out.println("Valores respectivos de 0 a 255 para R G B: ");
		R = sc.nextInt();
		G = sc.nextInt();
		B = sc.nextInt();
		
		System.out.println(R + G + B);
		sc.close();
	}
}
